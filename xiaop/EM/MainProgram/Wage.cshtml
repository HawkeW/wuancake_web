        <div class="div3">
            <div class="divWA">
                <input id="xiugai" type="button" value="修改" class="button"  />
                </div>
                        <div id="divtable">
            <div>
                <table id="table" class="bordered" style="width:100%">
                    <tr>
                        <th>序号</th>
                        <th>员工编号</th>
