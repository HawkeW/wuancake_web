                        <td>@sql["WageCard"]</td>
                        <td>@sql["MedicalIn"]</td>
                        <td>@sql["EmpIn"]</td>
                        <td>@sql["MaternityIn"]</td>
                        <td>@sql["UnemploymentIN"]</td>
                        <td>@sql["OldAgeIn"]</td>
                        <td>@sql["HousingPro"]</td>
                        <td>@sql["TraveSu"]</td>
                        <th class="td">修改</th>
                        <th class="th">删除</th>
                    </tr>
                    }
                    }
                </table>
            </div>
            <div class="divWA" style="width:100%;height:40px;text-align:center;">
